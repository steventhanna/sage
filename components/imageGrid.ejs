<div class="row">
  <% if (typeof images == "undefined" || images.length == 0) { %>
    <div class="col-lg-12">
      <center>
        <div class="error-main">
          <h1>Drag Files Here</h1>
          <p>
            To start a search, drag and drop image files here.
          </p>
        </div>
      </center>
    </div>
  <% } else { %>
    <% for (var i = 0; i < images.length; i++) { %>
      <div class="col-lg-3">
        <p>
          <%= images.fileName %>
        </p>
      </div>
    <% } %>
  <% } %>
</div>
